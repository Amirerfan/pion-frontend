    "@uniswap/token-lists": "^1.0.0-beta.30",
    "@uniswap/v3-periphery": "^1.4.1",
    "bignumber.js": "^9.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "typechain": "^8.2.0",
    "viem": "^1.0.5",
